    <!-- Intro Header -->
    <header class="intro">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <!-- <h1 class="brand-heading"></h1> -->
                        <a href="#watch" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a>
                        <p class="intro-text">Omega Seamaster</p>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Watch Section -->
    <section id="watch" class="container content-section text-center">
        <div class="watch-section">
            <div class="container">
            <hr>
              <div class="col-lg-3 col-sm-12 col-md-6"><%= image_tag("seamaster.png", :alt => "rss feed") %><br><%= 'seamaster 300'.upcase %></div>
              <div class="col-lg-3 col-sm-12 col-md-6"><%= image_tag("constellation.png", :alt => "rss feed") %><br><%= 'constellation'.upcase %></div>
              <div class="col-lg-3 col-sm-12 col-md-6"><%= image_tag("racing.png", :alt => "rss feed") %><br><%= 'racing'.upcase %></div>
              <div class="col-lg-3 col-sm-12 col-md-6"><%= image_tag("museum.png", :alt => "rss feed") %><br><%= 'museum'.upcase %></div>
            </div>
            <hr>
        </div>
    </section>

    <!-- Download Section -->
    <section id="download" class="content-section text-center">
        <div class="download-section">
            <div class="container">
            <div class="col-lg-8 col-lg-offset-2">
            <h2>Omega</h2>
            <p>OMEGA launced the original Seamaster 300 in 1957. An instant classic, it was admired by generations of divers and underwater professionals. More than half a century later, it has been completely upgraded and enhanced.</p>
            </div>
            <!-- <a href="http://taiwoayanleye.com/overboard/" class="btn btn-default btn-lg">Visit Registration Page</a> -->
            </div>
        </div>
    </section>


    <!-- Contact Section -->
    <section id="contact" class="container content-section text-center">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2>Omega Stores</h2>
                <p>LAGOS • Egbeda • Lekki • Victoria Island</p>
                <p><a href="mailto:feedback@startbootstrap.com">info@omega.com</a>
                </p>
                <ul class="list-inline banner-social-buttons">
                    <li>
                        <a href="https://twitter.com/stutern" class="btn btn-default btn-lg"><i class="fa fa-twitter fa-fw"></i> <span class="network-name">Twitter</span></a>
                    </li>
                    <li>
                        <a href="https://www.facebook.com/stuternpage" class="btn btn-default btn-lg"><i class="fa fa-facebook fa-fw"></i> <span class="network-name">Facebook</span></a>
                    </li>
                    <li>
                        <a href="https://www.linkedin.com/company/stutern" class="btn btn-default btn-lg"><i class="fa fa-linkedin fa-fw"></i> <span class="network-name">Linkedin</span></a>
                    </li>
                </ul>
            </div>
        </div>
    </section>

       <!-- Map Section -->
